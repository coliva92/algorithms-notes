\begin_layout Subsection
Transitividad
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $h:\mathbb{N}\to\mathbb{R}$
\end_inset

 una función asintóticamente positiva.
 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $f=\Theta(g)$
\end_inset

 y 
\begin_inset Formula $g=\Theta(h)$
\end_inset

, entonces 
\begin_inset Formula $f=\Theta(h)$
\end_inset

.
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $f=O(g)$
\end_inset

 y 
\begin_inset Formula $g=O(h)$
\end_inset

, entonces 
\begin_inset Formula $f=O(h)$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $f=\Omega(g)$
\end_inset

 y 
\begin_inset Formula $g=\Omega(h)$
\end_inset

, entonces 
\begin_inset Formula $f=\Omega(h)$
\end_inset

.
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $f=o(g)$
\end_inset

 y 
\begin_inset Formula $g=o(h)$
\end_inset

, entonces 
\begin_inset Formula $f=o(h)$
\end_inset

.
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $f=\omega(g)$
\end_inset

 y 
\begin_inset Formula $g=\omega(h)$
\end_inset

, entonces 
\begin_inset Formula $f=\omega(h)$
\end_inset

.
\end_layout

\begin_layout Subsection
Reflexividad
\end_layout

\begin_layout Standard
Las sig.
 igualdades se satisfacen porque 
\begin_inset Formula $f(n)=f(n)$
\end_inset

 para toda 
\begin_inset Formula $n$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}f & =\Theta(f)\\
f & =O(f)\\
f & =\Omega(f)
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Subsection
